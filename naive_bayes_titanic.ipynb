   ],
   "source": [
    "right_answer_count = 0\n",
    "#run the naive bayes function with every data point and compare with the answers and calculate the accuracy\n",
    "for i in range(len(survived_test)):\n",
    "    if pclass_test[i] == None or sex_test[i] == None or age_test[i] == None:\n",
    "        continue\n",
    "    answer = naive_bayes_titanic(pclass_test[i], sex_test[i], age_test[i])\n",
    "    if answer == survived_test[i]:\n",
    "        right_answer_count = right_answer_count + 1\n",
    "        \n",
    "print((right_answer_count/len(survived_test)) * 100, '%')"
   ]
  }
